~~ Sets the r0 register to 0 and then loops and increments it forever

.entry begin

::begin
xor r0 r0 r0
::loop
add r0 r0 zero(1)
jrz zero(rel loop) zero

ld r1 r0

xor r0 r1 zero(-1)

sub r0 zero r1

add r0 r1 zero(1)

sub r0 r1 zero(1)

jz r0 zero

jrz r0 zero

add r0 r1 zero


li r0 0b10111111111111111111101
lui r0 0b1011111111111101
lli r0 0b1011111111111101

lui r0 sts "bo"
lui r0 sts "ot"
